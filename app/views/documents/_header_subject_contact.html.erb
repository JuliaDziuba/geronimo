<% if !subject_contact['name'].nil? %>
	<%= subject_contact['name'] %><br />
<% end %>
<% if !subject_contact['phone'].nil? %>
	<%= subject_contact['phone'] %><br />
<% end %>
<% if !subject_contact['email'].nil? %>
	<%= subject_contact['email'] %><br />
<% end %>
<% if !subject_contact['address_street'].nil? %>
	<%= subject_contact['address_street'] %><br />
<% end %>
<% if !subject_contact['city'].nil? || !subject_contact['state'].nil? || !subject_contact['zipcode'].nil? %>
	<%= subject_contact['city'] %> <%= subject_contact['state'] %>, <%= subject_contact['zipcode'] %>
<% end %>