<%= form_for(venue, :html => { :class => "form-horizontal" }) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="row-fluid">   
    <div class="span5">
      <div class="control-group">
        <%= f.label :name, :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :name%>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :phone, :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :phone %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :email, :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :email %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :site, :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :site %>
        </div>
      </div>
    </div>
    <div class="span6 offset1">
      <div class="control-group">
        <%= f.label :address_street, "Street address", :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :address_street %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :address_city, "City", :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :address_city %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :address_state, "State", :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :address_state %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :address_zipcode, "Zip code", :class => "control-label" %>
        <div class="controls">
              <%= f.text_field :address_zipcode %>
        </div>
      </div>
    </div>
  </div>
  <div class="row-fluid">
    <div class="form-actions">
      <%= f.submit class: "btn" %>
    </div> 
  </div>
<% end %>