<ul class="list">
  <% venues.each do |venue| %>
    <li>
      <span class="venue-title"><%= link_to venue.name, venue %>
       	&nbsp; <%= link_to icon(:edit), edit_venue_path(venue) %>
      	&nbsp; <%= link_to icon(:trash), venue, method: :delete,
        	data: { confirm: "Delete #{venue.name}?" },
          title: "Delete #{venue.name}?" %></span><br />
          <span class = "list-description"><%= venue.venuecategory.name %><br />
      <span class="list-description">
        <%= render 'shared/venue_contact', venue: venue %>
          <br />
        <%= render 'shared/venue_address', venue: venue %>
      </span>
    </li>
  <% end %>
</ul>